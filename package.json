{
  "name": "react-chatbot",
  "version": "0.1.0",
  "private": true,
  "description": "React Chatbot",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "compile:prod": "better-npm-run compile:prod",
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "start:test": "better-npm-run start:test",
    "style:dev": "better-npm-run style:dev",
    "style:compile": "better-npm-run style:compile"
  },
  "betterScripts": {
    "compile": {
      "command": "node --max-old-space-size=4096 bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "npm run test && npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "start": {
      "command": "node --max-old-space-size=4096 bin/dev-server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "PORT": 3000,
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "start:test": {
      "command": "node --max-old-space-size=4096 bin/dev-server",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*",
        "PORT": 3000,
        "UV_THREADPOOL_SIZE": 100
      }
    },
    "test": {
      "command": "",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "1.0.1",
    "gulp-rename": "1.2.2",
    "html-webpack-plugin": "2.30.1",
    "imports-loader": "0.7.1",
    "intl": "^1.2.5",
    "ip": "1.1.5",
    "json-loader": "0.5.7",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-intl": "^2.4.0",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "3.7.2",
    "redux-persist": "^5.4.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.1",
    "babel-plugin-istanbul": "3.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "better-npm-run": "0.0.13",
    "css-loader": "0.26.4",
    "eslint": "3.19.0",
    "eslint-config-jquery": "1.0.1",
    "eslint-config-standard": "6.2.1",
    "eslint-config-standard-react": "4.3.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-jquery": "1.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "2.3.1",
    "express": "4.16.2",
    "file-loader": "0.9.0",
    "node-sass": "4.6.0",
    "nodemon": "1.12.1",
    "normalize.css": "5.0.0",
    "postcss-loader": "1.2.2",
    "sass-loader": "4.1.1",
    "string-replace-loader": "1.3.0",
    "style-loader": "0.13.2",
    "url-loader": "0.5.9",
    "webpack": "1.15.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.20.0"
  }
}
